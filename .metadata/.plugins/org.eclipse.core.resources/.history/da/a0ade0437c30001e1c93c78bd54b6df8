package fundamentos;

public class DesafioAritmeticos {

	public static void main(String[] args) {
	
		double dividendo1 = 6 * (3 + 2);
		double divisor1 = 3 * 2;
		
		double dividendo2 = (1 - 5) * (2 - 7);
		double divisor2 = 2;
		
		double elevandoDividendo1 = Math.pow(dividendo1, 2);
		double resultadoDivisao1 =  elevandoDividendo1 / divisor1;
		
		double resultadoDivisao2 = dividendo2 / divisor2;
		
		double elevandoResultadoDivisao2 = Math.pow(resultadoDivisao2, 2); 
		
		double dividendoGeral = (150 - 100);
		double resultadoDividendoGeral = Math.pow(dividendoGeral, 3);
		
		int divisorGeral = (int) Math.pow(10, 3);
		
		
		System.out.println(resultadoDividendoGeral/divisorGeral );
				
		
	}
	
}
