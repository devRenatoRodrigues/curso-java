package fundamentos;

public class NotacaoPonto {

	public static void main(String[] args) {
		double a = 2.3;
		String s = "Vai Palestra !!!";
		s = s.replace("Palestra", "Palmeiras");
		s = s.toUpperCase();
		s = s.concat("!!!");	
		System.out.println(s);
		String x = "Renato".toUpperCase();
		System.out.println(x);
		
	}
}
